<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ranningactivity"
    app:startDestination="@id/fragmentA">

    <fragment
        android:id="@+id/fragmentA"
        android:name="com.baysoftware.bayfit.HomeFragment"
        android:label=""
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_fragmentA_to_fragmentB"
            app:destination="@id/fragmentB" />
    </fragment>

    <fragment
        android:id="@+id/fragmentB"
        android:name="com.baysoftware.bayfit.TimerFragment"
        android:label="fragment_b"
        tools:layout="@layout/fragment_timer">

        <action
            android:id="@+id/action_fragmentB_to_fragmentA"
            app:destination="@id/fragmentA" />
        <action
            android:id="@+id/action_fragmentB_to_timerFramentUp"
            app:destination="@id/timerFramentUp" />
    </fragment>
    <fragment
        android:id="@+id/timerFramentUp"
        android:name="com.baysoftware.bayfit.TimerFramentUp"
        android:label="TimerFramentUp" >
        <action
            android:id="@+id/action_timerFramentUp_to_fragmentB"
            app:destination="@id/fragmentB" />
    </fragment>

</navigation>

